boolean chessBoardCellColor(String cell1, String cell2) {
    int col1 = cell1.charAt(0) - 'A' + 1;
    int row1 = cell1.charAt(1) - '0';
    int col2 = cell2.charAt(0) - 'A' + 1;
    int row2 = cell2.charAt(1) - '0';
    boolean check = false;
    if((col1 + row1) % 2 == 0 && (col2 + row2) % 2 == 0){
        check = true;
    }
    return check;
}
